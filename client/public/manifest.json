{
  "manifest_version": 3,
  "name": "Chrome Extension ",
  "description": "React Chrome Extension",
  "version": "1.0.0",

  "action": {
    "default_popup": "popup.html",
    "default_title": "Open"
  },
  "host_permissions":[
      "*://*.google.com/"
  ],
  "permissions": ["tabs","scripting", "https://localhost:5000/*", "storage","cookies"]
}
